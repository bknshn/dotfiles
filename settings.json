{
    "html.format.contentUnformatted": "pre, code, textarea, title, h1, h2, h3, h4, h5, h6, p", // タグ内の記述はフォーマットしない
    "html.format.extraLiners": "", // head, body, /html タグの前に改行を入れない
    "html.format.unformatted": null, // フレージング・コンテンツ（旧インライン要素のようなタグ）はフォーマットしない
    "editor.insertSpaces": true,
    "editor.tabSize": 4, // タブのサイズ変更
    "editor.minimap.enabled": false,
    "markdown-pdf.convertOnSave": false,
    "markdown-pdf.displayHeaderFooter": false,
    "window.zoomLevel": 0,
    // "vim.useSystemClipboard": true, // システムのclipboardにコピー
    "vim.visualstar": true,
    "vim.hlsearch": true,
    "vim.leader": "<space>",
    "vim.easymotion": true,
    "code-runner.runInTerminal": true,
    "code-runner.executorMap": {
        "cpp": "cd $dir && g++ -O0 -std=c++14 -D_MY_DEBUG -D_GLIBCXX_DEBUG $fileName && ./a.out",
        // "python": "cd $dir && source env/bin/activate && python $fileName",
        "python": "$pythonPath $fullFileName",
    },
    "code-runner.saveFileBeforeRun": true,
    "code-runner.preserveFocus": false,
    "workbench.statusBar.visible": true,
    "workbench.activityBar.visible": true,
    "workbench.sideBar.location": "left",
    "workbench.startupEditor": "newUntitledFile",
    "workbench.panel.defaultLocation": "right",
    "workbench.settings.editor": "json",
    "workbench.settings.openDefaultSettings": true,
    "python.pythonPath": "/usr/local/bin/python3",
    "cSpell.enabledLanguageIds": [
        "latex",
        "markdown",
    ],
    "[python]": {
        "editor.tabSize": 4,
    },
    "editor.acceptSuggestionOnCommitCharacter": false,
    "C_Cpp.updateChannel": "Insiders",
    "C_Cpp.intelliSenseCacheSize": 16,
    "python.linting.pylintEnabled": false,
    "python.linting.flake8Enabled": true,
    "python.linting.enabled": true,
    "editor.formatOnSave": true,
    "files.autoSave": "afterDelay",
    "editor.autoClosingOvertype": "always",
    "git.autofetch": true,
	"autoDocstring.docstringFormat": "google",
}